{
  "name": "onebytepls",
  "type": "module",
  "version": "1.0.0",
  "description": "Personal website for technical writing - One Byte Please",
  "scripts": {
    "dev:fe": "astro dev",
    "dev:full": "concurrently \"npm run dev:fe\" \"npm run dev:be\" --names \"astro,wrangler\" --prefix-colors \"cyan,magenta\"",
    "dev:be": "wrangler dev --port 8788",
    "build": "astro build",
    "preview": "astro preview",
    "astro": "astro",
    "lint": "eslint src/",
    "format": "prettier --write ."
  },
  "dependencies": {
    "@astrojs/react": "^4.4.2",
    "@astrojs/sitemap": "^3.6.0",
    "@tailwindcss/typography": "^0.5.19",
    "@tailwindcss/vite": "^4.1.18",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "astro": "^5.16.6",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "tailwindcss": "^4.1.18"
  },
  "devDependencies": {
    "concurrently": "^9.2.1",
    "wrangler": "^4.54.0"
  }
}
